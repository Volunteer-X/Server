{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/gateway/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/gateway/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/gateway",
  "projects": {
    "gateway": {
      "type": "application",
      "root": "apps/gateway",
      "entryFile": "main",
      "sourceRoot": "apps/gateway/src",
      "compilerOptions": {
        "tsConfigPath": "apps/gateway/tsconfig.app.json"
      }
    },
    "users": {
      "type": "application",
      "root": "apps/users",
      "entryFile": "main",
      "sourceRoot": "apps/users/src",
      "compilerOptions": {
        "tsConfigPath": "apps/users/tsconfig.app.json"
      }
    },
    "prisma": {
      "type": "library",
      "root": "libs/prisma",
      "entryFile": "index",
      "sourceRoot": "libs/prisma/src",
      "compilerOptions": {
        "tsConfigPath": "libs/prisma/tsconfig.lib.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "ping": {
      "type": "application",
      "root": "apps/ping",
      "entryFile": "main",
      "sourceRoot": "apps/ping/src",
      "compilerOptions": {
        "tsConfigPath": "apps/ping/tsconfig.app.json"
      }
    },
    "deletion-service": {
      "type": "application",
      "root": "apps/deletion-service",
      "entryFile": "main",
      "sourceRoot": "apps/deletion-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/deletion-service/tsconfig.app.json"
      }
    },
    "activity": {
      "type": "application",
      "root": "apps/activity",
      "entryFile": "main",
      "sourceRoot": "apps/activity/src",
      "compilerOptions": {
        "tsConfigPath": "apps/activity/tsconfig.app.json"
      }
    }
  }
}